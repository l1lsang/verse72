{
  "expo": {
    "name": "일용할 양식",
    "slug": "daily-bread",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "bundleIdentifier": "com.quokka.dailybread"
    },

    "android": {
      "package": "com.quokka.dailybread"
    },

    "extra": {
      "eas": {
        "projectId": "c3609fab-af53-4466-b35d-7efa26a3f4b2"
      }
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "https://devrepo.kakao.com/nexus/content/groups/public/"
            ]
          }
        }
      ]
    ]
  }
}
